{
  "name": "xun-components",
  "version": "1.0.0",
  "author": "pengjielee",
  "scripts": {
    "dev": "dumi dev",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "pt": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "pt-watch": "onchange 'src/**/*.{js,jsx,ts,tsx,less,md,json,scss}' -- prettier --write {{changed}}",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "new:comp": "node scripts/newcomp.js",
    "build:comp": "rollup -c"
  },
  "main": "dist/index.js",
  "module": "es/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@react-spring/web": "^9.3.0",
    "@use-gesture/react": "^10.1.1",
    "classnames": "^2.3.1",
    "install": "^0.13.0",
    "npm": "^8.1.0",
    "postcss": "^8.3.11",
    "react": "^16.12.0 || ^17.0.0",
    "react-svg": "^14.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.0.5",
    "chalk": "^4.1.2",
    "dumi": "^1.0.17",
    "dumi-theme-mobile": "^1.1.13",
    "father-build": "^1.17.2",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.0.0",
    "inquirer": "^8.1.2",
    "lint-staged": "^10.0.7",
    "node-sass": "^6.0.1",
    "onchange": "^7.1.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.4.2",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "files": [
    "dist",
    "es",
    "umd"
  ]
}
